{
  "name": "compare",
  "version": "0.0.1",
  "description": "product comparison",
  "main": "server.js",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "start": "nodemon server.js",
    "storybook": "start-storybook -p 6006 -c ./storybook",
    "test": "jest --colors",
    "test:coverage": "jest --coverage --colors --no-cache",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "prelint": "npm run check:types",
    "format": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx --fix . && stylelint \"**/*.css\" --fix",
    "lint:eslint": "eslint --ext .js,.ts,.tsx --max-warnings 0 .",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "check:types": "tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "npm run format",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "author": "happyden",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
